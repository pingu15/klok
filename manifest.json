{
    "name": "klok",
    "description": "A Productivity Visual",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "cookies.js",
            "popup.js",
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "alarms", 
        "cookies",
        "notifications",
     	"webRequest",
   	    "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png"
    }
}
